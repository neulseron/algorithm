#include<iostream>
#include<vector>
using namespace std;

int main(int argc, char** argv)
{
	int test_case;
	int T;
	cin>>T;
	for(test_case = 1; test_case <= T; ++test_case)
	{
		int N = 0;
        cin>>N;
        vector<int> popCard;
        int sum = 0, goMaxNumber = 0;
        int Go = 0, Stop = 0;
        for(int idx = 0; idx < N; ++idx)
        {
            int cardNumber;
            cin>>cardNumber;
            if(cardNumber == 1)
            {
                sum += 11;
                popCard.push_back(11);
            }
            else if(10 <= cardNumber)
            {
                sum += 10;
                popCard.push_back(10);
            }
            else
            {
            	sum += cardNumber;
                popCard.push_back(cardNumber);
            }
        }
        int newCard = 2;
        for(int idx = 0; idx < 13; ++idx)
        {
            if(sum + newCard <= 21)
            {
                ++Go;
                goMaxNumber = idx;
            }
            else
            {
                ++Stop;
            }
            if(newCard < 10)
            {
            	++newCard;
            }
            if(11 == idx)
            {
                ++newCard;
            }
        }
        for(int idx = 0; idx < N; ++idx)
        {
            if(goMaxNumber < popCard[idx])
            {
                --Stop;
            }
            else
            {
                --Go;
            }
        }
        cout<<"#"<<test_case<<" ";
        if(Go < Stop)
        {
            cout<<"STOP"<<endl;
        }
		else
        {
            cout<<"GAZUA"<<endl;
        }
	}
	return 0;//정상종료시 반드시 0을 리턴해야합니다.
}
